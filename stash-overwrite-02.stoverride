name: 俄区分流规则
desc: 俄区分流规则，若有俄区节点请开启

proxy-groups:
  - name: "俄罗斯分组"
    type: fallback
    url: "http://www.google.com/generate_204"
    interval: 300
    include-all: true
    filter: "(?i)(俄罗斯|Russia|Russian)"

    
rule-providers:
  ru-list:
    behavior: classical
    format: text
    url: https://raw.githubusercontent.com/q1343135559/url-rule/main/ru.list
    interval: 86400

rules:
  - RULE-SET,ru-list,俄罗斯分组